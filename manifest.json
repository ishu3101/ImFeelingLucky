{
  "manifest_version": 2,

  "name": "I'm Feeling Lucky",
  "version": "1.0.0",

  "description": 
    "Search selected text using the I'm Feeling Lucky Google Search",

  "icons": { 
    "16" : "icon/icon16.png",
    "32" : "icon/icon32.png",
    "48" : "icon/icon48.png",
    "128" : "icon/icon128.png"
  },

  "permissions": ["contextMenus"],
  
  "background": {
    "scripts": ["index.js"],
    "persistent": false
  }
}
